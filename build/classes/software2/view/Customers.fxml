<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="850.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="software2.view.CustomersController">
   <children>
      <VBox layoutX="14.0" layoutY="168.0" prefHeight="650.0" prefWidth="149.0">
         <children>
            <Button fx:id="calendarButton" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#calendarView" prefHeight="162.0" prefWidth="154.0" text="Calendar">
               <font>
                  <Font size="19.0" />
               </font>
            </Button>
            <Button fx:id="customersButton" mnemonicParsing="false" onAction="#customersView" prefHeight="162.0" prefWidth="154.0" text="Customers">
               <font>
                  <Font size="19.0" />
               </font>
            </Button>
            <Button fx:id="reportsButton" mnemonicParsing="false" onAction="#reportsView" prefHeight="162.0" prefWidth="154.0" text="Reports">
               <font>
                  <Font size="19.0" />
               </font>
            </Button>
            <Button fx:id="logoutButton" mnemonicParsing="false" onAction="#logoutButton" prefHeight="162.0" prefWidth="154.0" text="Log Out">
               <font>
                  <Font size="19.0" />
               </font>
            </Button>
         </children>
      </VBox>
      <TableView fx:id="customersTable" layoutX="175.0" layoutY="168.0" prefHeight="650.0" prefWidth="1000.0">
         <columns>
            <TableColumn fx:id="nameColumn" prefWidth="143.0" text="Name" />
            <TableColumn fx:id="phoneColumn" minWidth="0.0" prefWidth="125.0" text="Phone" />
            <TableColumn fx:id="addressColumn" prefWidth="175.0" text="Address" />
            <TableColumn fx:id="address2Column" minWidth="0.0" prefWidth="150.0" text="Address 2" />
            <TableColumn fx:id="cityColumn" prefWidth="152.0" text="City" />
            <TableColumn fx:id="postalCodeColumn" prefWidth="95.0" text="Postal Code" />
            <TableColumn fx:id="countryColumn" prefWidth="156.0" text="Country" />
         </columns>
      </TableView>
      <Button fx:id="createCustomerButton" layoutX="333.0" layoutY="105.0" mnemonicParsing="false" onAction="#createCustomerView" prefHeight="49.0" prefWidth="136.0" text="Create Customer">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="modifyCustomerButton" layoutX="568.0" layoutY="105.0" mnemonicParsing="false" onAction="#modifyCustomerView" prefHeight="49.0" prefWidth="136.0" text="Modify Customer" />
      <Button fx:id="deleteCustomerButton" layoutX="800.0" layoutY="105.0" mnemonicParsing="false" onAction="#deleteCUstomertHandler" prefHeight="49.0" prefWidth="136.0" text="Delete Customer" />
   </children>
</AnchorPane>
